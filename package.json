{
  "name": "bike-finder",
  "version": "1.0.0",
  "description": "thief's got burning hat",
  "main": "server.js",
  "scripts": {
    "tsc": "tsc",
    "start": "rm -rf ./dist && tsc && nodemon ./dist/index.js",
    "start:local": "rm -rf ./dist && tsc && node -r dotenv/config ./dist/index.js dotenv_config_path=.env.local",
    "test": "mocha -r ts-node/register src/**/*.spec.ts --exit",
    "test:coverage": "nyc mocha -r ts-node/register src/**/*.spec.ts --exit",
    "dev": "ts-node-dev --respawn --transpileOnly ./src/index.ts"
  },
  "author": "Alex",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "mysql2": "^1.6.5",
    "sequelize": "^4.42.1",
    "sequelize-typescript": "^0.6.7"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/chai-http": "^3.0.5",
    "@types/dotenv": "^6.1.0",
    "@types/es6-promise": "^3.3.0",
    "@types/express": "^4.16.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.9.6",
    "@types/sequelize": "^4.27.38",
    "chai": "^4.2.0",
    "chai-http": "4.2.0",
    "mocha": "^6.0.2",
    "nodemon": "^1.18.10",
    "nyc": "^13.3.0",
    "reflect-metadata": "^0.1.13",
    "ts-node-dev": "^1.0.0-pre.32",
    "tslint": "^5.13.1",
    "typescript": "^3.3.3333"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
